<div id="divSampleQA" class="col-12 col-md-12 col-lg-12" style="display:none;">
    <div class="card">
        <div class="col-12">
            <form id="modal-test-part" role="form" class="">
                <input type="hidden" id="hdnQaSUPNAME" />
                <input type="hidden" id="hdnQaDOCNO" />
                <input type="hidden" id="hdnQaPARTNAME" />
                <input type="hidden" id="hdnQaORDNAME" />
                <input type="hidden" id="hdnQACODE" />
                <input type="hidden" id="hdnLOCATION" />
                <input type="hidden" id="hdnQA" />
                <input type="hidden" id="hdnQaREPETITION" />
                <input type="hidden" id="hdnQaSAMPQUANT" />
                <div class="card-body">
                    <div class="row">
                        <div class="form-group col-md-2 col-12">
                            <label style="font-weight:bolder;">קוד בדיקה</label>
                            <div class="col-sm-1">
                                <label type="text" id="txtQACODE" class="form-label"></label>
                            </div>
                        </div>
                        <div class="form-group col-md-4 col-12">
                            <label style="font-weight:bolder;">תאור בדיקה</label>
                            <div class="col-sm-12">
                                <label type="text" id="txtQADES"></label>
                            </div>
                        </div>
                        <div class="form-group col-md-2 col-12">
                            <label style="font-weight:bolder;">מיקום</label>
                            <div class="col-sm-3 form-label">
                                <label type="text" id="txtQaLOCATION"></label>
                            </div>
                        </div>
                        <div class="form-group col-md-4 col-12">
                            <label style="font-weight:bolder;">בדיקה - טקסט</label>
                            <div class="col-sm-4 form-label">
                                <label type="text" id="txtQaSHR_TEST" class=""></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-2 col-12">
                            <label style="font-weight:bolder;">תוצאת מינימום</label>
                            <div class="col-sm-1">
                                <label type="text" id="txtQaRESULTMIN" class="form-label"></label>
                            </div>
                        </div>
                        <div class="form-group col-md-4 col-12">
                            <label style="font-weight:bolder;">תוצאת מקסימום</label>
                            <div class="col-sm-12">
                                <label type="text" id="txtQaRESULTMAX">11.23</label>
                            </div>
                        </div>
                        <div class="form-group col-md-2 col-12">
                            <label style="font-weight:bolder;">חזרות</label>
                            <div class="col-sm-3 form-label">
                                <label type="text" id="txtQaREPETITION"></label>
                            </div>
                        </div>
                        <div class="form-group col-md-4 col-12">
                            <label style="font-weight:bolder;">תוצאתית</label>
                            <div class="col-sm-4 form-label">
                                <input type="checkbox" id="txtQaRESULTANT" class="big-checkbox" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-2 col-12">
                            <label style="font-weight:bolder;">תוצאה נדרשת</label>
                            <div class="col-sm-1">
                                <label type="text" id="txtQaREQUIRED_RESULT" class="form-label"></label>
                            </div>
                        </div>
                        <div class="form-group col-md-4 col-12">
                            <label style="font-weight:bolder;">גודל המדגם</label>
                            <div class="col-sm-12">
                                <label type="text" id="txtQaSAMPQUANT"></label>
                            </div>
                        </div>
                        <div class="form-group col-md-2 col-12">
                            <label style="font-weight:bolder;">תקין</label>
                            <div class="col-sm-3 form-label">
                                <input type="checkbox" id="txtQaNORMAL" class="big-checkbox" />
                            </div>
                        </div>
                        <div class="form-group col-md-4 col-12">
                            <label style="font-weight:bolder;">תאור כלי מדידה</label>
                            <div class="col-sm-4">
                                <label type="text" name="MEASUREDES" id="txtQaMEASUREDES"></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-2 col-12">
                            <label style="font-weight:bolder;">תוצאת בדיקה</label>
                            <input class="form-control" type="number" pattern="^\d*(\.\d{0,6})?$" step="any" name="RESULT" id="txtQaRESULT" />
                        </div>
                        <div class="form-group col-md-10 col-12">
                            <label style="font-weight:bolder;">הערה</label>
                            <input class="form-control" type="text" name="REMARK" id="txtQaREMARK" maxlength="120" />
                        </div>
                    </div>
                    <div class="row" id="testRepitition" style="display:none;">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <h5 class="section-title" style="color:#01416F">סבב בדיקות</h5>
                                </div>
                                <div class="row" id="repititionTest"></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <h5 class="section-title" style="color:#01416F">קבצים מצורפים</h5>
                                    <p class="section-lead">ניתן להעלות מספר קבצים במקביל</p>
                                </div>
                                <div class="card-body">
                                    <div class="fallback">
                                        <input name="attachments" id="attachments" type="file" multiple />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>