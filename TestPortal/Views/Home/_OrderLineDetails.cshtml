<div id="divOrdLineDetails" class="col-12 col-md-12 col-lg-12" style="display:none;">
    <div id="accOrdLine">
        <div class="card">
            <div class="col-12">
                <div class="card-header" id="headingTow_Main">
                    <h5 class="mb-12">
                        <span style="color:#34395e;cursor:pointer;" data-toggle="collapse" data-target="#collapseTow_Main" aria-expanded="true" aria-controls="collapseTow_Main">
                            <i class="fas fa-puzzle-piece" style="font-size: 20px;"></i> Product information
                        </span>
                    </h5>
                    @*<h4><i class="fas fa-puzzle-piece" style="font-size: 20px;"></i> Product information</h4>*@
                    <input type="hidden" id="hdnOrdId" value="" />
                    <input type="hidden" id="hdnPrdId" value="" />
                    <input type="hidden" id="hdnSUPNAME" value="" />
                    <input type="hidden" id="hdnORDNAME" value="" />
                    <input type="hidden" id="hdnIsNewDocument" value="-1" />
                </div>
                <div id="collapseTow_Main" class="collapse show" aria-labelledby="headingTow_Main" data-parent="#accOrdLine">
                    <div class="card-body">
                        <div class="row">
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Line #</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_LINE"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Catalog number</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_PARTNAME"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-3 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Pro. description</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_PDES"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Quantity</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_TQUANT"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Balance for supply</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_TBALANCE"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Supply date</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_pageREQDATE" style="display:none;"></label>
                                    <input type="date" id="txt_pageREQDATE" style="display:none;" />
                                </div>
                            </div>
                            <div class="form-group col-md-3 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">req. supply date</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_REQDATE2">REQDATE2</label>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label " style="color:#01416F; font-weight:bolder;">Serial</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_SERIALNAME"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Action</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_ACTNAME"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-4 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Drow</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_SHR_DRAW"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Manufacturer catalog</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_SHR_MNFPARTNAME"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Manufacturer name</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_SHR_MNFDES"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Product edition</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_REVNAME"></label>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-2 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Product tree edition</label>
                                <div class="col-sm-12">
                                    <label class="control-label" id="lbl_SHR_SERIAL_REVNUM"></label>
                                </div>
                            </div>
                            <div class="form-group col-md-4 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Reason for rejection</label>
                                <div class="col-sm-12">
                                    <select class="form-control" id="combo_DELAYREASON" style="display:none;" onchange="comboDelayReasonOnChange();"></select>
                                    <label class="control-label" id="lbl_DELAYREASON" style="display:none;"></label>
                                </div>
                            </div>
                        </div>
                        <div class="row" id="div_ReasonRejection">
                            <div class="form-group col-md-12 col-12">
                                <label class="control-label" style="color:#01416F; font-weight:bolder;">Other reason for rejection</label>
                                <div class="col-sm-12">
                                    <input class="form-control" id="txt_ReasonRejection" maxlength="120" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="accOrdLineMsg">
        <div class="card">
            <div class="col-12">
                <div class="card-header" id="headingTow">
                    <h5 class="mb-12">
                        <span style="color:#34395e;cursor:pointer;" data-toggle="collapse" data-target="#collapseTow" aria-expanded="true" aria-controls="collapseTow">
                            <i class="fas fa-exclamation-circle" style="font-size: 20px;"></i>  Order line comment
                        </span>
                    </h5>
                </div>
                @*<div class="card-header">
                <h4><i class="fas fa-exclamation-circle" style="font-size: 20px;"></i>Order line comment</h4>
            </div>*@
                <div id="collapseTow" class="collapse show" aria-labelledby="headingTow" data-parent="#accOrdLineMsg">
                    <div class="card-body">
                        <div class="row">
                            <div class="form-group col-md-12 col-12">
                                <div class="col-sm-12" id="orderLineAlert"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        document.getElementById('divOrdLineDetails').style.display = 'inline';
    });
</script>
